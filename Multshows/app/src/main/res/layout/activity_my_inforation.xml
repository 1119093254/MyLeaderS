<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/My_Inforation_parentLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:fresco="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:fitsSystemWindows="true"
    android:background="@color/background_grey"
    tools:context=".Activity.My_Information_Activity">
    <RelativeLayout
        android:id="@+id/My_Inforation_returnLayout"
        style="@style/top_APP"
        android:background="@drawable/layout_border_bottom1_white">
        <ImageView
            android:id="@+id/My_Inforation_return"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:layout_centerVertical="true"
            android:src="@drawable/return_arrow"/>
        <TextView
            android:id="@+id/My_Inforation_TopTitle"
            style="@style/text_ww_20_text_grey_2"
            android:layout_centerInParent="true"
            android:text="完善个人信息"/>
        <!--<TextView
            android:id="@+id/My_Inforation_save"
            style="@style/text_ww_17_black"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="16dp"
            android:text="保存"/>-->
    </RelativeLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <RelativeLayout
                android:id="@+id/My_Inforation_header"
                android:layout_width="match_parent"
                android:layout_height="76dp"
                android:background="@drawable/layout_border_bottom1_white">
                <TextView
                    android:id="@+id/My_Inforation_headerImage"
                    style="@style/text_ww_16_black"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="16dp"
                    android:text="头像"/>

                <ImageView
                    android:id="@+id/My_Inforation_image1"
                    android:layout_width="8dp"
                    android:layout_height="13dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="16dp"
                    android:src="@drawable/item_shipping_address_gonext"/>
                <com.facebook.drawee.view.SimpleDraweeView
                    android:id="@+id/My_Inforation_header_pic"
                    android:layout_width="54dp"
                    android:layout_height="54dp"
                    android:layout_toLeftOf="@id/My_Inforation_image1"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="16dp"
                    fresco:failureImage="@drawable/info_photoyou"
                    fresco:placeholderImage="@drawable/info_photoyou"
                    fresco:roundAsCircle="true"
                    />
            </RelativeLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="16dp"
                android:background="@drawable/layout_border_top1_bottom1_white">
                <RelativeLayout
                    android:id="@+id/My_Inforation_Phone"
                    android:layout_width="match_parent"
                    android:layout_height="49dp"
                    android:layout_marginLeft="16dp"
                    android:background="@drawable/layout_border_bottom1_white">
                    <TextView
                        style="@style/text_ww_16_black"
                        android:layout_centerVertical="true"
                        android:text="账号"/>
                    <TextView
                        android:id="@+id/My_Inforation_Phone_text"
                        style="@style/text_ww_16_light_grey"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="17dp"
                        android:text=""/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/My_Inforation_nickName"
                    android:layout_width="match_parent"
                    android:layout_height="49dp"
                    android:layout_marginLeft="16dp"
                    android:background="@drawable/layout_border_bottom1_white">
                    <TextView
                        style="@style/text_ww_16_black"
                        android:layout_centerVertical="true"
                        android:text="昵称"/>
                    <ImageView
                        android:id="@+id/My_Inforation_image2"
                        android:layout_width="8dp"
                        android:layout_height="13dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="16dp"
                        android:src="@drawable/item_shipping_address_gonext"/>
                    <TextView
                        android:id="@+id/My_Inforation_name_text"
                        style="@style/text_ww_16_light_grey"
                        android:layout_centerVertical="true"
                        android:layout_toLeftOf="@id/My_Inforation_image2"
                        android:layout_marginRight="8dp"
                        android:text=""/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/My_Inforation_birthday"
                    android:layout_width="match_parent"
                    android:layout_height="49dp"
                    android:layout_marginLeft="16dp"
                    android:background="@drawable/layout_border_bottom1_white">
                    <TextView
                        style="@style/text_ww_16_black"
                        android:layout_centerVertical="true"
                        android:text="生日"/>
                    <ImageView
                        android:id="@+id/My_Inforation_image4"
                        android:layout_width="8dp"
                        android:layout_height="13dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="16dp"
                        android:src="@drawable/item_shipping_address_gonext"/>
                    <TextView
                        android:id="@+id/My_Inforation_birthday_text"
                        style="@style/text_ww_16_light_grey"
                        android:layout_centerVertical="true"
                        android:layout_toLeftOf="@id/My_Inforation_image4"
                        android:layout_marginRight="8dp"
                        android:text=""/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/My_Inforation_sex"
                    android:layout_width="match_parent"
                    android:layout_height="49dp"
                    android:layout_marginLeft="16dp"
                    android:background="@drawable/layout_border_bottom1_white">
                    <TextView
                        style="@style/text_ww_16_black"
                        android:layout_centerVertical="true"
                        android:text="性别"/>
                    <ImageView
                        android:id="@+id/My_Inforation_image5"
                        android:layout_width="8dp"
                        android:layout_height="13dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="16dp"
                        android:src="@drawable/item_shipping_address_gonext"/>
                    <TextView
                        android:id="@+id/My_Inforation_sex_text"
                        style="@style/text_ww_16_light_grey"
                        android:layout_centerVertical="true"
                        android:layout_toLeftOf="@id/My_Inforation_image5"
                        android:layout_marginRight="8dp"
                        android:text=""/>
                </RelativeLayout>
               <!-- <RelativeLayout
                    android:id="@+id/My_Inforation_relationship"
                    android:layout_width="match_parent"
                    android:layout_height="49dp"
                    android:layout_marginLeft="16dp"
                    android:background="@drawable/layout_border_bottom1_white">
                    <TextView
                        style="@style/text_ww_16_black"
                        android:layout_centerVertical="true"
                        android:text="和宝贝关系"/>
                    <ImageView
                        android:id="@+id/My_Inforation_image6"
                        android:layout_width="8dp"
                        android:layout_height="13dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="16dp"
                        android:src="@drawable/item_shipping_address_gonext"/>
                </RelativeLayout>-->
                <RelativeLayout
                    android:id="@+id/My_Inforation_realName"
                    android:layout_width="match_parent"
                    android:layout_height="49dp"
                    android:layout_marginLeft="16dp"
                    android:background="@drawable/layout_border_bottom1_white">
                    <TextView
                        style="@style/text_ww_16_black"
                        android:layout_centerVertical="true"
                        android:text="实名认证"/>
                    <ImageView
                        android:id="@+id/My_Inforation_image7"
                        android:layout_width="8dp"
                        android:layout_height="13dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="16dp"
                        android:src="@drawable/item_shipping_address_gonext"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/My_Inforation_address"
                    android:layout_width="match_parent"
                    android:layout_height="49dp"
                    android:background="@drawable/layout_border_bottom1_white"
                    android:layout_marginLeft="16dp">
                    <TextView
                        style="@style/text_ww_16_black"
                        android:layout_centerVertical="true"
                        android:text="收货地址"/>
                    <ImageView
                        android:id="@+id/My_Inforation_image8"
                        android:layout_width="8dp"
                        android:layout_height="13dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="16dp"
                        android:src="@drawable/item_shipping_address_gonext"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/My_invite_Code"
                    android:layout_width="match_parent"
                    android:layout_height="49dp"
                    android:layout_marginLeft="16dp">
                    <TextView
                        style="@style/text_ww_16_black"
                        android:layout_centerVertical="true"
                        android:id="@+id/myInvite"
                        android:text="我的邀请码："/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:textColor="@color/bg_black"
                        android:id="@+id/MyInviteCode"
                        android:layout_toRightOf="@+id/myInvite"
                        android:layout_marginLeft="5dp"
                        android:textIsSelectable="true"
                        android:text=""/>
                    <ImageView
                        android:id="@+id/My_Inforation_image9"
                        android:layout_width="8dp"
                        android:layout_height="13dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="16dp"
                        android:src="@drawable/item_shipping_address_gonext"/>
                </RelativeLayout>
            </LinearLayout>


        </LinearLayout>
    </ScrollView>

</LinearLayout>
