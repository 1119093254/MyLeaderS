<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Rechage_Web_ParentsLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:fitsSystemWindows="true"
    android:background="@color/background_grey"
    tools:context="com.multshows.Activity.ReChangeWeb_Activity">
    <RelativeLayout
        android:id="@+id/Rechage_Web_returnLayout"
        style="@style/top_APP"
        android:background="@drawable/layout_border_bottom1_white">
        <ImageView
            android:id="@+id/Rechage_Web_return"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:layout_centerVertical="true"
            android:src="@drawable/return_arrow"/>

        <TextView
            android:id="@+id/Rechage_Web_TopTitle"
            style="@style/text_ww_20_text_grey_2"
            android:layout_centerInParent="true"
            android:text="银联充值"/>
    </RelativeLayout>
    <WebView
        android:id="@+id/Rechage_Web_webView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</LinearLayout>
