<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="63dp"
    android:background="@drawable/layout_border_bottom1_white">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="17dp"
        android:orientation="vertical">
        <TextView
            android:id="@+id/my_TradingList_items_Title"
            style="@style/text_ww_16_black"
            android:text="标题"/>
        <TextView
            android:id="@+id/my_TradingList_items_Time"
            style="@style/text_ww_11_light_grey"
            android:layout_marginTop="11dp"
            android:text="0000-00-00"/>
    </LinearLayout>
    <TextView
        android:id="@+id/my_TradingList_items_money"
        style="@style/text_ww_16_black"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_marginRight="17dp"
        android:text="+¥0.00"/>
</RelativeLayout>
</LinearLayout>