package com.multshows.Beans;

import java.io.Serializable;
import java.util.ArrayList;
import java.util.List;

/**
 * 任务标签实体类
 */
public class TaskClassTag implements Serializable{
    private String Id;//加密Id
    private String Name;//任务标签名称
    private String ClassId;//加密：任务分类Id
    private int RecordTime;//记录时间
    private String RecordTimeStr;//记录时间字符串
    private String Icon;//图标
    private String Title;//标题
    private String Content;//内容
    private int Count;//此标签下完成任务宝宝的数量
    private List<String> UserImgList=new ArrayList<>();//用户头像集合
    private int IsDefined;//1:是;0：否

    public TaskClassTag() {
    }

    public String getId() {
        return Id;
    }

    public void setId(String id) {
        Id = id;
    }

    public String getName() {
        return Name;
    }

    public void setName(String name) {
        Name = name;
    }

    public String getClassId() {
        return ClassId;
    }

    public void setClassId(String classId) {
        ClassId = classId;
    }

    public int getRecordTime() {
        return RecordTime;
    }

    public void setRecordTime(int recordTime) {
        RecordTime = recordTime;
    }

    public String getRecordTimeStr() {
        return RecordTimeStr;
    }

    public void setRecordTimeStr(String recordTimeStr) {
        RecordTimeStr = recordTimeStr;
    }

    public String getIcon() {
        return Icon;
    }

    public void setIcon(String icon) {
        Icon = icon;
    }

    public String getTitle() {
        return Title;
    }

    public void setTitle(String title) {
        Title = title;
    }

    public String getContent() {
        return Content;
    }

    public void setContent(String content) {
        Content = content;
    }

    public int getCount() {
        return Count;
    }

    public void setCount(int count) {
        Count = count;
    }

    public List<String> getUserImgList() {
        return UserImgList;
    }

    public void setUserImgList(List<String> userImgList) {
        UserImgList = userImgList;
    }

    public int getIsDefined() {
        return IsDefined;
    }

    public void setIsDefined(int isDefined) {
        IsDefined = isDefined;
    }
}
