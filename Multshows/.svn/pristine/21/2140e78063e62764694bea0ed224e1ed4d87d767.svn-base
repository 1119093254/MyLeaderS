<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:id="@+id/number_bottom_Parents"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:layout_alignParentBottom="true"
              >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_alignParentBottom="true"
        android:background="@color/background_grey">
        <RelativeLayout
            style="@style/top_APP"
            android:background="@drawable/layout_border_top1_bottom1_white">
            <ImageView
                android:id="@+id/Rechange_PassWord_Return"
                android:layout_width="49dp"
                android:layout_height="49dp"
                android:src="@drawable/close_page01"/>
            <TextView
                style="@style/text_ww_17_black"
                android:layout_centerInParent="true"
                android:text="输入密码"/>
            <TextView
                android:id="@+id/Rechange_PassWord_Money"
                style="@style/text_ww_17_black"
                android:layout_alignParentRight="true"
                android:layout_marginRight="16dp"
                android:layout_centerVertical="true"
                android:visibility="gone"
                android:text="¥0.0"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="135dp">
            <!--android:cursorVisible="false"是否获取焦点-->
            <EditText
                android:id="@+id/Rechange_PassWord_EditText"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:inputType="textPassword"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:layout_marginTop="28dp"
                android:singleLine="true"
                android:background="@drawable/shape_white_9"
                android:paddingLeft="6dp"

                android:maxLength="20"
                android:hint="请输入支付密码"/>
            <TextView
                android:id="@+id/Rechange_PassWord_Forget"
                style="@style/text_ww_14_text_green"
                android:layout_alignRight="@id/Rechange_PassWord_EditText"
                android:layout_below="@id/Rechange_PassWord_EditText"
                android:layout_marginTop="11dp"
                android:text="忘记密码?"/>

        </RelativeLayout>
        <TextView
            android:id="@+id/Rechange_PassWord_OK"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/text_green"
            android:textSize="17dp"
            android:textColor="@color/white"
            android:gravity="center"
            android:text="确定"/>
    </LinearLayout>

