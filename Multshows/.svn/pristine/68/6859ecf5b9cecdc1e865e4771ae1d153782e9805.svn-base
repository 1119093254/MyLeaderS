package com.multshows.Beans;

import java.io.Serializable;

/**
 * 添加评论
 */
public class ShowsCommentAdd implements Serializable {
    String Id;//加密评论id
    String UserId;//加密评论者id
    String TargetUserId;//加密被回复者id（一级评论时传Authorld）
    String AuthorId;//加密作者id
    String ForeignId;//加密相应表的id
    String ReplyId;//加密回复评论id(被回复的评论的id)
    int  Type;//1 作品评论 2 活动评论3 公益评论
    String Content;//评论内容

    public ShowsCommentAdd() {
    }

    public String getId() {
        return Id;
    }

    public void setId(String id) {
        Id = id;
    }

    public String getUserId() {
        return UserId;
    }

    public void setUserId(String userId) {
        UserId = userId;
    }

    public String getTargetUserId() {
        return TargetUserId;
    }

    public void setTargetUserId(String targetUserId) {
        TargetUserId = targetUserId;
    }

    public String getAuthorId() {
        return AuthorId;
    }

    public void setAuthorId(String authorId) {
        AuthorId = authorId;
    }

    public String getForeignld() {
        return ForeignId;
    }

    public void setForeignld(String foreignld) {
        ForeignId = foreignld;
    }

    public String getReplyId() {
        return ReplyId;
    }

    public void setReplyId(String replyId) {
        ReplyId = replyId;
    }

    public int getType() {
        return Type;
    }

    public void setType(int type) {
        Type = type;
    }

    public String getContent() {
        return Content;
    }

    public void setContent(String content) {
        Content = content;
    }
}
