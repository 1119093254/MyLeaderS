<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Setting_ParentsLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:fitsSystemWindows="true"
    android:background="@color/background_grey"
    tools:context="com.multshows.Activity.Setting_Activity">
    <RelativeLayout
        android:id="@+id/Setting_returnLayout"
        style="@style/top_APP"
        android:background="@drawable/layout_border_bottom1_white">
        <ImageView
            android:id="@+id/Setting_return"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:layout_centerVertical="true"
            android:src="@drawable/return_arrow"/>
        <TextView
            android:id="@+id/Setting_TopTitle"
            style="@style/text_ww_20_text_grey_2"
            android:layout_centerInParent="true"
            android:text="设置"/>
    </RelativeLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="8dp"
                android:background="@drawable/layout_border_top1_bottom1_white">
                <RelativeLayout
                    android:id="@+id/Setting_changePassWord"
                    android:layout_width="match_parent"
                    android:layout_height="53dp"
                    android:layout_marginLeft="17dp"
                    android:background="@drawable/layout_border_bottom1_white"
                    >
                    <TextView
                        style="@style/text_ww_15_text_grey_2"
                        android:layout_centerVertical="true"
                        android:text="修改登录密码"/>
                    <ImageView
                        android:layout_width="8dp"
                        android:layout_height="13dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="17dp"
                        android:layout_centerVertical="true"
                        android:src="@drawable/set_more_icon"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/Setting_RechargePassWord"
                    android:layout_width="match_parent"
                    android:layout_height="53dp"
                    android:layout_marginLeft="17dp"
                    android:background="@drawable/layout_border_bottom1_white"
                    >
                    <TextView
                        style="@style/text_ww_15_text_grey_2"
                        android:layout_centerVertical="true"
                        android:text="设置支付密码"/>
                    <ImageView
                        android:layout_width="8dp"
                        android:layout_height="13dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="17dp"
                        android:layout_centerVertical="true"
                        android:src="@drawable/set_more_icon"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/Setting_switchNumber"
                    android:layout_width="match_parent"
                    android:layout_height="53dp"
                    android:layout_marginLeft="17dp"
                    >
                    <TextView
                        style="@style/text_ww_15_text_grey_2"
                        android:layout_centerVertical="true"
                        android:text="修改切换密码"/>
                    <ImageView
                        android:layout_width="8dp"
                        android:layout_height="13dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="17dp"
                        android:layout_centerVertical="true"
                        android:src="@drawable/set_more_icon"/>
                </RelativeLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/Setting_informCenter"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="8dp"
                android:background="@drawable/layout_border_top1_bottom1_white">
                <RelativeLayout
                    android:id="@+id/Setting_inform"
                    android:layout_width="match_parent"
                    android:layout_height="53dp"
                    android:layout_marginLeft="17dp"
                    android:background="@drawable/layout_border_bottom1_white"
                    >
                    <TextView
                        style="@style/text_ww_15_text_grey_2"
                        android:layout_centerVertical="true"
                        android:text="通知"/>
                    <ImageView
                        android:id="@+id/Setting_inform_image"
                        android:layout_width="57dp"
                        android:layout_height="35dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="16dp"
                        android:layout_centerVertical="true"
                        android:src="@drawable/switch_off"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/Setting_callUs"
                    android:layout_width="match_parent"
                    android:layout_height="53dp"
                    android:layout_marginLeft="17dp"
                    >
                    <TextView
                        style="@style/text_ww_15_text_grey_2"
                        android:layout_centerVertical="true"
                        android:text="关于我们"/>
                    <ImageView
                        android:layout_width="8dp"
                        android:layout_height="13dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="17dp"
                        android:layout_centerVertical="true"
                        android:src="@drawable/set_more_icon"/>
                </RelativeLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/Setting_informBottom"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="8dp"
                android:background="@drawable/layout_border_top1_bottom1_white">
                <RelativeLayout
                    android:id="@+id/Setting_clear"
                    android:layout_width="match_parent"
                    android:layout_height="53dp"
                    android:layout_marginLeft="17dp"
                    android:background="@drawable/layout_border_bottom1_white"
                    >
                    <TextView
                        style="@style/text_ww_15_text_grey_2"
                        android:layout_centerVertical="true"
                        android:text="清理缓存"/>
                    <TextView
                        android:id="@+id/Setting_clear_text"
                        style="@style/text_ww_15_light_grey"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="17dp"
                        android:layout_centerVertical="true"
                        android:text="0M"
                        />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/Setting_suggestion"
                    android:layout_width="match_parent"
                    android:layout_height="53dp"
                    android:layout_marginLeft="17dp"
                    android:background="@drawable/layout_border_bottom1_white"
                    >
                    <TextView
                        style="@style/text_ww_15_text_grey_2"
                        android:layout_centerVertical="true"
                        android:text="意见反馈"/>
                    <ImageView
                        android:layout_width="8dp"
                        android:layout_height="13dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="17dp"
                        android:layout_centerVertical="true"
                        android:src="@drawable/set_more_icon"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/Setting_versionNumber"
                    android:layout_width="match_parent"
                    android:layout_height="53dp"
                    android:layout_marginLeft="17dp"
                    >
                    <TextView
                        style="@style/text_ww_15_text_grey_2"
                        android:layout_centerVertical="true"
                        android:text="版本号"/>
                    <ImageView
                        android:id="@+id/Setting_versionNumber_image"
                        android:layout_width="8dp"
                        android:layout_height="13dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="17dp"
                        android:layout_centerVertical="true"
                        android:src="@drawable/set_more_icon"/>
                    <TextView
                        android:id="@+id/Setting_versionNumber_text"
                        style="@style/text_ww_15_light_grey"
                        android:layout_toLeftOf="@id/Setting_versionNumber_image"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="9dp"
                       />
                </RelativeLayout>


            </LinearLayout>
            <Button
                android:id="@+id/Setting_button"
                android:layout_width="match_parent"
                android:layout_height="39dp"
                style="?android:attr/borderlessButtonStyle"
                android:layout_margin="17dp"
                android:background="@drawable/shape_green_4"
                android:textColor="@color/white"
                android:textSize="20dp"
                android:text="退出登录"/>

        </LinearLayout>
    </ScrollView>

</LinearLayout>
