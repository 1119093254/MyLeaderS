<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/my_fragment_ok_timeIcon"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:src="@drawable/icon_cashing"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="28dp"/>
                <LinearLayout
                    android:id="@+id/my_fragment_ok_Money"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@id/my_fragment_ok_timeIcon"
                    android:layout_marginTop="22dp"
                    android:gravity="center_vertical">
                    <TextView
                        style="@style/text_ww_20_text_grey_2"
                        android:text="-¥"/>
                    <TextView
                        android:id="@+id/My_Fragment_OK_money"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:textSize="34sp"
                        android:textColor="@color/text_grey_2"
                        android:text="0.0"/>
                </LinearLayout>
                <TextView
                    android:id="@+id/My_Fragment_OK_BankHint"
                    style="@style/text_ww_13_light_grey"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@id/my_fragment_ok_Money"
                    android:layout_marginTop="16dp"
                    android:layout_marginBottom="17dp"
                    android:text="提现成功,请等待银行处理"/>
            </RelativeLayout>
            <LinearLayout
                android:id="@+id/My_Fragment_OK_MoneyLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginLeft="17dp"
                android:layout_marginRight="17dp"
                android:visibility="gone">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                       style="@style/text_ww_14_text_grey_2"
                        android:text="提现金额:"/>
                    <TextView
                        android:id="@+id/My_Fragment_OK_AllMoney"
                        style="@style/text_ww_14_light_grey"
                        android:layout_alignParentRight="true"
                        android:text="¥0.0"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp">
                    <TextView
                        style="@style/text_ww_14_text_grey_2"
                        android:text="手续费:"/>
                    <TextView
                        android:id="@+id/My_Fragment_OK_Fei"
                        style="@style/text_ww_14_light_grey"
                        android:layout_alignParentRight="true"
                        android:text="-¥0.0"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="46dp"
                    android:layout_marginTop="14dp"
                    android:background="@drawable/layout_border_top1_alight">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true">
                        <TextView
                            style="@style/text_ww_16_text_grey_2"
                            android:text="实际到账金额:"/>
                        <TextView
                            android:id="@+id/My_Fragment_OK_RealMoney1"
                            style="@style/text_ww_16_text_grey_2"
                            android:layout_marginLeft="14dp"
                            android:text="¥0"/>
                        <TextView
                            android:id="@+id/My_Fragment_OK_RealMoney2"
                            style="@style/text_ww_14_text_grey_2"
                            android:text=".0"/>
                    </LinearLayout>

                </RelativeLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="@color/white">
                <RelativeLayout
                    android:id="@+id/My_Fragment_OK_BankLayout"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_marginLeft="17dp"
                    android:background="@drawable/layout_border_bottom1_white">
                    <TextView
                        style="@style/text_ww_16_light_grey"
                        android:layout_centerVertical="true"
                        android:text="提现到"/>
                    <TextView
                        android:id="@+id/My_Fragment_OK_Bank"
                        style="@style/text_ww_16_text_grey_2"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="17dp"
                        android:text=""/>
                    <TextView
                        android:id="@+id/My_Fragment_OK_BankType"
                        style="@style/text_ww_16_light_grey"
                        android:layout_toLeftOf="@id/My_Fragment_OK_Bank"
                        android:layout_marginRight="8dp"
                        android:layout_centerVertical="true"
                        android:text=""/>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_marginLeft="17dp">
                    <TextView
                        style="@style/text_ww_16_light_grey"
                        android:layout_centerVertical="true"
                        android:text="提现时间"/>
                    <TextView
                        android:id="@+id/My_Fragment_OK_Time"
                        style="@style/text_ww_16_text_grey_2"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="17dp"
                        android:text=""/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/My_Fragment_OK_ChoseBankLayout"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_marginLeft="17dp"
                    android:visibility="gone"
                    android:background="@drawable/layout_border_top1_white">
                    <TextView
                        style="@style/text_ww_16_light_grey"
                        android:layout_centerVertical="true"
                        android:text="失败原因"/>
                    <!--<ImageView
                        android:id="@+id/My_Fragment_OK_ChoseBank"
                        android:layout_width="8dp"
                        android:layout_height="13dp"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="17dp"
                        android:src="@drawable/item_shipping_address_gonext"/>-->
                    <TextView
                        android:id="@+id/My_Fragment_OK_ChoseBankType"
                        style="@style/text_ww_16_black"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="8dp"
                        android:layout_centerVertical="true"
                        android:text=""/>
                </RelativeLayout>
            </LinearLayout>
            <TextView
                android:id="@+id/My_Fragment_OK_HintLayout"
                style="@style/text_ww_11_light_grey"
                android:layout_marginLeft="17dp"
                android:layout_marginTop="4dp"
                android:visibility="gone"
                android:text="备注: 已将提现金额退回至账户余额"/>
            <Button
                android:id="@+id/My_Fragment_OK_OKBtn"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                style="?android:attr/borderlessButtonStyle"
                android:background="@drawable/shape_green_4"
                android:layout_marginTop="34dp"
                android:layout_marginLeft="17dp"
                android:layout_marginRight="17dp"
                android:textColor="@color/white"
                android:textSize="18sp"
                android:text="完成"/>
        </LinearLayout>
    </ScrollView>
</LinearLayout>