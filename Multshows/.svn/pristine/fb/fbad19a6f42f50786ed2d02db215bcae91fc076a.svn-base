<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/My_MyWallet_Fragment_ParentsLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/background_grey"
    android:fitsSystemWindows="true"
    tools:context=".Activity.My_Information_Change_Activity">
    <RelativeLayout
        android:id="@+id/My_MyWallet_Fragment_returnLayout"
        style="@style/top_APP"
        android:background="@drawable/layout_border_bottom1_white">
        <ImageView
            android:id="@+id/My_MyWallet_Fragment_return"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:layout_centerVertical="true"
            android:src="@drawable/return_arrow"/>
        <TextView
            android:id="@+id/My_MyWallet_Fragment_title"
            style="@style/text_ww_20_text_grey_2"
            android:layout_centerInParent="true"
            android:text=""/>
    </RelativeLayout>
    <FrameLayout
        android:id="@+id/My_MyWallet_Fragment_fragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</LinearLayout>
