<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:fresco="http://schemas.android.com/apk/res-auto"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="63dp"
                android:layout_marginTop="9dp"
                android:background="@color/white">
                <com.facebook.drawee.view.SimpleDraweeView
                    android:id="@+id/Bound_BankList_Item_icon"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="17dp"
                    fresco:failureImage="@drawable/defult"
                    fresco:placeholderImage="@drawable/defult"
                    />
                <TextView
                    android:id="@+id/Bound_BankList_Item_Name"
                    style="@style/text_ww_16_black"
                    android:layout_toRightOf="@id/Bound_BankList_Item_icon"
                    android:layout_alignTop="@id/Bound_BankList_Item_icon"
                    android:layout_marginLeft="16dp"
                    android:text=""/>
                <TextView
                    android:id="@+id/Bound_BankList_Item_Information"
                    style="@style/text_ww_13_light_grey"
                    android:layout_toRightOf="@id/Bound_BankList_Item_icon"
                    android:layout_alignBottom="@id/Bound_BankList_Item_icon"
                    android:layout_marginLeft="16dp"
                    android:text=""/>
                <ImageView
                    android:id="@+id/Bound_BankList_Item_Choose"
                    android:layout_width="12dp"
                    android:layout_height="8dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/icon_hook"/>
            </RelativeLayout>
</LinearLayout>