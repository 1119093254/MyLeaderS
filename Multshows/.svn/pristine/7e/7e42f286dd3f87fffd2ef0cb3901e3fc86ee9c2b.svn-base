<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:orientation="vertical">
    <RelativeLayout
        android:id="@+id/home_myHomePage_myPhotoAlbums_item_layout"
        android:layout_width="match_parent"
        android:layout_height="94dp"
        android:background="@drawable/layout_border_bottom1_white">

        <ImageView
            android:id="@+id/home_myHomePage_myPhotoAlbums_item_image"
            android:layout_width="68dp"
            android:layout_height="67dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="17dp"
            android:background="@color/background_grey"/>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="13dp"
            android:layout_toRightOf="@id/home_myHomePage_myPhotoAlbums_item_image">

            <TextView
                android:id="@+id/home_myHomePage_myPhotoAlbums_item_AlbumsName"
                style="@style/text_ww_17_grey"
                android:text="我的相册"/>

            <TextView
                style="@style/text_ww_17_text_grey_2"
                android:text="("
                />

            <TextView
                android:id="@+id/home_myHomePage_myPhotoAlbums_item_photoNumber"
                style="@style/text_ww_17_grey"
                android:text="0"
                />

            <TextView
                style="@style/text_ww_17_text_grey_2"
                android:text=")"/>
        </LinearLayout>

        <ImageView
            android:layout_width="8dp"
            android:layout_height="14dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="17dp"
            android:src="@drawable/item_shipping_address_gonext"/>
    </RelativeLayout>
</LinearLayout>