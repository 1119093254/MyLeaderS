                value1="[em:m00"+i+"]";
                aaa=aaa.replace(value1,key1);
            }
            if(i<100&&i>=10){
                value1="[em:m0"+i+"]";
                aaa=aaa.replace(value1,key1);
            }
            if(i>=100) {
                value1="[em:m"+i+"]";
                aaa=aaa.replace(value1,key1);
            }
        }
        return aaa;
    }


