    public static SpannableString Emojichange(Activity activity, String context){
        //消息具体内容
        String zhengze = "emm[0-9]e|emm[0-9]{2}e|emm[1][0][1-3]e|emc[0-9]c|emc[0-9]{2}c|emc[1][0-3][0-9]c";
        SpannableString spannableString = ExpressionUtil.getExpressionString(activity, context, zhengze);
        return spannableString;
    }
