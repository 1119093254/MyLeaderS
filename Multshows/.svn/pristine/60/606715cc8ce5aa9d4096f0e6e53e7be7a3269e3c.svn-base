package com.multshows.Beans;

import java.io.Serializable;

/**
 * 评论，赞我的，公益 公用beans对象
 */
public class Friends_AllList_Beans implements Serializable{
    private String userId;//用户id
    private String userName;//用户昵称
    private String userPic;//用户头像

    private String SSUserId;//说说用户id
    private String SSUserName;//说说用户昵称
    private String SSUserPic;//说说用户头像
    private String SSContent;//说说内容
    private String toUserName;//对方用户昵称
    private String comment;//评论内容
    private String time;//操作时间

    public Friends_AllList_Beans(String userId, String userName, String userPic, String SSUserId,
                                 String SSUserName, String SSUserPic, String SSContent, String toUserName,
                                 String comment, String time) {
        this.userId = userId;
        this.userName = userName;
        this.userPic = userPic;
        this.SSUserId = SSUserId;
        this.SSUserName = SSUserName;
        this.SSUserPic = SSUserPic;
        this.SSContent = SSContent;
        this.toUserName = toUserName;
        this.comment = comment;
        this.time = time;
    }

    public String getUserId() {
        return userId;
    }

    public void setUserId(String userId) {
        this.userId = userId;
    }

    public String getUserName() {
        return userName;
    }

    public void setUserName(String userName) {
        this.userName = userName;
    }

    public String getUserPic() {
        return userPic;
    }

    public void setUserPic(String userPic) {
        this.userPic = userPic;
    }

    public String getSSUserId() {
        return SSUserId;
    }

    public void setSSUserId(String SSUserId) {
        this.SSUserId = SSUserId;
    }

    public String getSSUserName() {
        return SSUserName;
    }

    public void setSSUserName(String SSUserName) {
        this.SSUserName = SSUserName;
    }

    public String getSSUserPic() {
        return SSUserPic;
    }

    public void setSSUserPic(String SSUserPic) {
        this.SSUserPic = SSUserPic;
    }

    public String getSSContent() {
        return SSContent;
    }

    public void setSSContent(String SSContent) {
        this.SSContent = SSContent;
    }

    public String getToUserName() {
        return toUserName;
    }

    public void setToUserName(String toUserName) {
        this.toUserName = toUserName;
    }

    public String getComment() {
        return comment;
    }

    public void setComment(String comment) {
        this.comment = comment;
    }

    public String getTime() {
        return time;
    }

    public void setTime(String time) {
        this.time = time;
    }
}
