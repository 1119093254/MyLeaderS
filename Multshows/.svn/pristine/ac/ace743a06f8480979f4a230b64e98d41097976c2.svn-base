<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:fresco="http://schemas.android.com/apk/res-auto"
              android:id="@+id/my_fragment_no_ParentLayout"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
        <RelativeLayout
            android:id="@+id/my_fragment_no_BoundBankCord"
            android:layout_width="match_parent"
            android:layout_height="58dp"
            android:background="@color/white">
            <ImageView
                android:id="@+id/my_fragment_no_BankIcon"
                android:layout_width="28dp"
                android:layout_height="28dp"
                android:layout_marginLeft="17dp"
                android:layout_centerVertical="true"
                android:src="@drawable/item_bankcard"/>
            <com.facebook.drawee.view.SimpleDraweeView
                android:id="@+id/my_fragment_no_BankIcon2"
                android:layout_width="28dp"
                android:layout_height="28dp"
                android:layout_marginLeft="17dp"
                android:layout_centerVertical="true"
                android:visibility="gone"
                fresco:failureImage="@drawable/info_photoyou"
                fresco:placeholderImage="@drawable/info_photoyou"
                />
            <TextView
                android:id="@+id/my_fragment_no_BankName"
                style="@style/text_ww_16_text_grey_2"
                android:layout_centerVertical="true"
                android:layout_marginLeft="60dp"
                android:text="提现到银行卡"/>
            <ImageView
                android:id="@+id/my_fragment_no_moreIcon"
                android:layout_width="8dp"
                android:layout_height="13dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="17dp"
                android:src="@drawable/more_icon"/>
            <TextView
                android:id="@+id/my_fragment_no_BountHint"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@id/my_fragment_no_moreIcon"
                android:layout_marginRight="14dp"
                android:gravity="right"
                android:textSize="13sp"
                android:textColor="@color/text_black"
                android:textColorHint="@color/light_grey"
                android:hint="点击绑定银行卡"/>
        </RelativeLayout>
    <RelativeLayout
        android:id="@+id/my_fragment_no_BoundBankCord_Baby"
        android:layout_width="match_parent"
        android:layout_height="58dp"
        android:background="@color/white"
        android:visibility="gone">
        <ImageView
            android:id="@+id/my_fragment_no_Baby_BankIcon"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:layout_marginLeft="17dp"
            android:layout_centerVertical="true"
            android:src="@drawable/item_mom_cash"/>
        <com.facebook.drawee.view.SimpleDraweeView
            android:id="@+id/my_fragment_no_Baby_BankIcon2"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:layout_marginLeft="17dp"
            android:layout_centerVertical="true"
            android:visibility="gone"
            fresco:failureImage="@drawable/info_photoyou"
            fresco:placeholderImage="@drawable/info_photoyou"
            />
        <TextView
            android:id="@+id/my_fragment_no_Baby_BankName"
            style="@style/text_ww_16_text_grey_2"
            android:layout_centerVertical="true"
            android:layout_marginLeft="60dp"
            android:text="提现到"/>
        <ImageView
            android:id="@+id/my_fragment_no_Baby_moreIcon"
            android:layout_width="8dp"
            android:layout_height="13dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="17dp"
            android:src="@drawable/more_icon"/>
        <TextView
            android:id="@+id/my_fragment_no_Baby_BountHint"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@id/my_fragment_no_Baby_moreIcon"
            android:layout_marginRight="14dp"
            android:gravity="right"
            android:textSize="13sp"
            android:textColor="@color/text_black"
            android:textColorHint="@color/light_grey"
            android:hint="请选择提转出位置"/>
    </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:orientation="vertical"
        android:background="@color/white">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:orientation="vertical">
            <TextView
                style="@style/text_ww_16_text_grey_2"
                android:layout_marginLeft="17dp"
                android:layout_marginTop="13dp"
                android:layout_marginBottom="8dp"
                android:text="提现金额"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:layout_marginLeft="17dp"
                android:background="@drawable/layout_border_bottom1_white">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="20sp"
                    android:textColor="@color/text_grey_2"
                    android:text="¥"/>
                <EditText
                    android:id="@+id/my_fragment_no_EditText"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:inputType="numberDecimal"
                    android:maxLines="1"
                    android:layout_marginLeft="16dp"
                    android:background="@null"
                    android:gravity="center_vertical"
                    android:textSize="20sp"
                    android:textColor="@color/text_grey_2"
                    android:textColorHint="@color/button_grey"
                    android:hint="输入提现金额"
                    />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingTop="14dp"
            android:paddingLeft="17dp">
            <TextView
                android:id="@+id/my_fragment_no_HintMoneyText"
                style="@style/text_ww_11_light_grey"
                android:visibility="gone"
                android:text="手续费=银行服务手续费1元/笔"/>
            <TextView
                style="@style/text_ww_11_light_grey"
                android:layout_marginTop="8dp"
                android:text="充值金额不可提现"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/my_fragment_no_HintLayout"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginLeft="17dp"
            android:gravity="center_vertical">
            <TextView
                style="@style/text_ww_13_light_grey"
                android:text="可提现"/>
            <TextView
                android:id="@+id/my_fragment_no_CanKitting"
                style="@style/text_ww_13_light_grey"
                android:text="0.0"/>
            <TextView
                style="@style/text_ww_13_light_grey"
                android:text=","/>
            <TextView
                android:id="@+id/my_fragment_no_AllKitting"
                style="@style/text_ww_13_text_green"
                android:text="全部提现"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/my_fragment_no_HintLayout2"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginLeft="17dp"
            android:gravity="center_vertical"
            android:visibility="gone">
            <TextView
                android:id="@+id/my_fragment_no_KittingHint"
                style="@style/text_ww_13_yellow"
                android:text="输入金额超过可提现金额"/>
        </LinearLayout>

    </LinearLayout>
    <Button
        android:id="@+id/my_fragment_no_KittingBtn"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        style="?android:attr/borderlessButtonStyle"
        android:background="@drawable/shape_button_grey_4"
        android:layout_marginTop="34dp"
        android:layout_marginLeft="17dp"
        android:layout_marginRight="17dp"
        android:textColor="@color/white"
        android:textSize="18sp"
        android:text="提现"/>
</LinearLayout>