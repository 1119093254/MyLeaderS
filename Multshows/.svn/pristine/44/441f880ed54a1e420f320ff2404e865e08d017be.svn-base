package com.multshows.Beans;

import java.io.Serializable;

/**
 * 群的个人设置
 */
public class PersonalSetting implements Serializable{
    private int NotifyType;//消息提醒类型 1正常提醒 2免打扰
    private String Alias;//在本群的昵称
    private int IsShowAlias;//是否显示群成员昵称 0不显示 1显示

    public PersonalSetting() {
    }

    public int getNotifyType() {
        return NotifyType;
    }

    public void setNotifyType(int notifyType) {
        NotifyType = notifyType;
    }

    public String getAlias() {
        return Alias;
    }

    public void setAlias(String alias) {
        Alias = alias;
    }

    public int getIsShowAlias() {
        return IsShowAlias;
    }

    public void setIsShowAlias(int isShowAlias) {
        IsShowAlias = isShowAlias;
    }
}
