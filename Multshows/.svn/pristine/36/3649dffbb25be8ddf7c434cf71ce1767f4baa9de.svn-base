<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/My_Kitting_ParentsLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:background="@color/background_grey"
    android:orientation="vertical"
    tools:context="com.multshows.Activity.My_Kitting_Activity">
    <RelativeLayout
        android:id="@+id/My_Kitting_returnLayout"
        style="@style/top_APP"
        android:background="@drawable/layout_border_bottom1_white">
        <ImageView
            android:id="@+id/My_Kitting_return"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:layout_centerVertical="true"
            android:src="@drawable/return_arrow"/>
        <TextView
            android:id="@+id/My_Kitting_TopTitle"
            style="@style/text_ww_20_text_grey_2"
            android:layout_centerInParent="true"
            android:text="提现"/>
    </RelativeLayout>
    <FrameLayout
        android:id="@+id/My_Kitting_Fragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</LinearLayout>
