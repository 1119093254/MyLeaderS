<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/add_NewAddress_ParentsLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:orientation="vertical"
    android:background="@color/background_grey"
    tools:context=".Activity.Add_NewAddress_Activity">
    <RelativeLayout
        android:id="@+id/add_NewAddress_returnLayout"
        style="@style/top_APP"
        android:background="@drawable/layout_border_bottom1_white">

        <ImageView
            android:id="@+id/add_NewAddress_return"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:layout_centerVertical="true"
            android:src="@drawable/return_arrow"/>

        <TextView
            android:id="@+id/add_NewAddress_TopTitle"
            style="@style/text_ww_20_text_grey_2"
            android:layout_centerInParent="true"
            android:text="新建收货地址"/>
        <TextView
            android:id="@+id/add_NewAddress_save"
            style="@style/text_ww_17_grey"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="18dp"
            android:text="保存"/>
    </RelativeLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="16dp"
                android:orientation="vertical"
                android:background="@drawable/layout_border_top1_bottom1_white">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="53dp"
                    android:background="@drawable/layout_border_bottom1_white">
                    <TextView
                        android:id="@+id/add_NewAddress_nametext"
                        style="@style/text_ww_16_black"
                        android:layout_centerVertical="true"
                        android:text="收货人:"/>
                    <EditText
                        android:singleLine="true"
                        android:id="@+id/add_NewAddress_Name"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@null"
                        android:layout_marginLeft="90dp"
                        android:textSize="16sp"
                        android:textColorHint="@color/text_white_c"
                        android:hint="请填写收货人姓名"/>
                </RelativeLayout>
               <!-- <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="53dp"
                    android:background="@drawable/layout_border_top1_bottom1_white">
                    <RadioGroup
                        android:id="@+id/add_NewAddress_RadioGroup"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:orientation="horizontal">
                        <RadioButton
                            android:id="@+id/add_NewAddress_RadioButton1"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:button="@null"
                            android:drawableLeft="@drawable/checkbox_state"
                            android:drawablePadding="12dp"
                            android:checked="true"
                            android:textColor="@color/text_black"
                            android:textSize="16sp"
                            android:text="先生"/>
                        <RadioButton
                            android:id="@+id/add_NewAddress_RadioButton2"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:button="@null"
                            android:layout_marginLeft="45dp"
                            android:drawableLeft="@drawable/checkbox_state"
                            android:drawablePadding="12dp"
                            android:textColor="@color/text_black"
                            android:textSize="16sp"
                            android:text="女士"/>
                    </RadioGroup>
                </RelativeLayout>-->
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="53dp"
                    android:background="@drawable/layout_border_bottom1_white">
                    <TextView
                        android:id="@+id/add_NewAddress_place_Other"
                        style="@style/text_ww_16_black"
                        android:layout_centerVertical="true"
                        android:text="地址别名:"/>
                    <EditText
                        android:singleLine="true"
                        android:id="@+id/add_NewAddress_place_OtherLayout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@null"
                        android:layout_marginLeft="90dp"
                        android:textSize="16sp"
                        android:textColorHint="@color/text_white_c"
                        android:hint="请输入地址别名(公司或住宅)"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="53dp"
                    android:background="@drawable/layout_border_bottom1_white">
                    <TextView
                        android:id="@+id/add_NewAddress_phonetext"
                        style="@style/text_ww_16_black"
                        android:layout_centerVertical="true"
                        android:text="电话:"/>
                    <EditText
                        android:singleLine="true"
                        android:id="@+id/add_NewAddress_Phone"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@null"
                        android:layout_marginLeft="90dp"
                        android:inputType="number"
                        android:textSize="16sp"
                        android:textColorHint="@color/text_white_c"
                        android:hint="填写收货手机号码"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/add_NewAddress_schoolLayout"
                    android:layout_width="match_parent"
                    android:layout_height="53dp"
                    android:background="@drawable/layout_border_bottom1_white">
                    <TextView
                        android:id="@+id/add_NewAddress_schooltext"
                        style="@style/text_ww_16_black"
                        android:layout_centerVertical="true"
                        android:text="省市区:"/>
                    <ImageView
                        android:id="@+id/add_NewAddress_schooltext_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="90dp"
                        android:src="@drawable/item_shipping_address_local"/>
                    <TextView
                        android:id="@+id/add_NewAddress_placeText"
                        style="@style/text_ww_16_text_black_3"
                        android:layout_toRightOf="@id/add_NewAddress_schooltext_2"
                        android:layout_centerVertical="true"
                        android:textColorHint="@color/text_white_c"
                        android:hint="点击选择"/>
                    <ImageView
                        android:layout_width="8dp"
                        android:layout_height="13dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="17dp"
                        android:layout_centerVertical="true"
                        android:src="@drawable/set_more_icon"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="53dp"
                    android:background="@drawable/layout_border_bottom1_white">
                    <TextView
                        android:id="@+id/add_NewAddress_hometext"
                        style="@style/text_ww_16_black"
                        android:layout_centerVertical="true"
                        android:text="详细地址:"/>
                    <EditText
                        android:singleLine="true"
                        android:id="@+id/add_NewAddress_Men"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@null"
                        android:layout_marginLeft="90dp"
                        android:textSize="16sp"
                        android:textColorHint="@color/text_white_c"
                        android:hint="详细地址"/>
                </RelativeLayout>

            </LinearLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="53dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/layout_border_top1_bottom1_white">
                <TextView
                    style="@style/text_ww_16_black"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="16dp"
                    android:text="设为默认"/>
                <ImageView
                    android:id="@+id/add_NewAddress_image"
                    android:layout_width="57dp"
                    android:layout_height="35dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="18dp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/switch_off"/>

            </RelativeLayout>
        </LinearLayout>
    </ScrollView>



</LinearLayout>
